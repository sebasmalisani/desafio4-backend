<div class="titulo">Productos</div>
<div class="listaProductos">
  {{#each listaproductos}}
    <article class="container">
      <div class="card">
        <div class="img">
          <img src="{{thumbnail}}" width="150" />
        </div>
        <div class="content">
          <h2>{{title}}</h2>
          <div class="size">
            <h3>{{description}}</h3>
            <h4> Stock: {{stock}}</h4>
          </div>
          <div>
            <h3>Price: {{price}}</h3>
          </div>
        </div>
      </div>

    </article>
  {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>